{
  "name": "jereap",
  "version": "0.0.1",
  "description": "Jepara Rest API boilerplate - starter kit - skeleton",
  "main": "src/index.js",
  "author": "M Sadam Sadewa <me@mssadewa.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "cross-env NODE_ENV=test mocha --recursive tests",
    "dev": "cross-env NODE_ENV=development nodemon src/index.js",
    "start": "pm2 start ecosystem.config.json --no-daemon",
    "prepare:db:test": "cross-env NODE_ENV=test && node src/script/prepare_database.js",
    "prepare:db:dev": "cross-env NODE_ENV=development && node src/script/prepare_database.js",
    "prepare:db:production": "cross-env NODE_ENV=production && node src/script/prepare_database.js",
    "seed:master:test": "cross-env NODE_ENV=test && node src/script/seed_master.js",
    "seed:master:dev": "cross-env NODE_ENV=dev && node src/script/seed_master.js",
    "seed:master:production": "cross-env NODE_ENV=production && node src/script/seed_master.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.20.1",
    "chai": "^4.3.7",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-session": "^1.17.3",
    "helmet": "^6.0.0",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "mocha": "^10.2.0",
    "morgan": "^1.10.0",
    "mysql2": "^2.3.3",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pm2": "^5.2.2",
    "role-acl": "^4.5.4",
    "sequelize": "^6.25.8",
    "winston": "^3.8.2",
    "winston-transport": "^4.5.0",
    "xss-clean": "^0.1.1"
  },
  "devDependencies": {
    "cross-env": "^7.0.3",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.3"
  }
}
